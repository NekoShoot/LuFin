{
  "name": "lufin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "echo '🚀 개발 환경(Development) 모드로 실행 중: Postman Mock API' && vite",
    "dev:prod": "echo '🔥 프로덕션(Production) 모드로 실행 중: 실제 백엔드 API' && vite --mode production",
    "build": "echo '⚡ 기본 빌드 실행 중' && tsc -b && vite build",
    "build:dev": "echo '🚀 개발 환경(Development) 모드로 빌드 중: Postman Mock API' && tsc -b && vite build --mode development",
    "build:prod": "echo '🔥 프로덕션(Production) 모드로 빌드 중: 실제 백엔드 API' && tsc -b && vite build --mode production",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "check-build": "tsc --noEmit",
    "fix": "npm run lint:fix && npm run format && npm run check-build && npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@headlessui/react": "^2.2.0",
