    "@tailwindcss/vite": "^4.0.15",
    "@types/moment": "^2.11.29",
    "axios": "^1.6.0",
    "chart.js": "^4.4.8",
    "framer-motion": "^12.6.3",
    "iconsax-react": "^0.0.8",
    "moment": "^2.30.1",
    "pretendard": "^1.3.9",
    "react": "^19.0.0",
    "react-calendar": "^5.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.0.0",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "tailwind-merge": "^3.0.2",
    "tailwind-scrollbar-hide": "^2.0.0",
    "tailwindcss": "^4.0.15",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
